@model Ucle.Models.EmailViewModel

@{
    ViewBag.Title = "Kickstart";  
}

<!-- ============================================================= SECTION – INTRODUCTION ============================================================= -->

<div class="container inner-top-sm inner-bottom-sm">
    <div class="row">
        <div class="col-md-11 col-sm-11 center-block text-center">
            <header>
                <h1>VC Fund</h1>
                <h3>We invest in early-stage startups by funding them with non-equity growth capital. Each year, we receive over 60 applications, analyse their business plans, and interview them to assess their potential. The best startups are then invited to pitch their ideas at our final round events, where we invite a panel of expert judges.
                </h3>
            </header>
        </div><!-- /.col -->
    </div><!-- /.row -->
</div><!-- /.container -->


<!-- ============================================================= SECTION – VIDEO ============================================================= -->

<section id="portfolio-post">
